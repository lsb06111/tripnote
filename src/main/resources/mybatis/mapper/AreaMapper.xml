<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.example.tripnote.dao.AreaDAO">
<select id="listParentLoc"  resultType="AreaDTO">
	SELECT
    	*
	FROM
	    area
	WHERE
	    parent_id IS NULL or area_name='서울'
</select>
</mapper>